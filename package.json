{
  "name": "icu-loader",
  "version": "2.0.0",
  "description": "A package to load full ICU data in Node.js environment with JavaScript Intl API",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "lint": "snazzy ./src/*.js index.js; true",
    "lint:fix": "npm run build && snazzy ./src/*.js index.js --fix; true",
    "stats-loc": "echo 'Source code stats:' && sloc --format cli-table ./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msudgh/icu-loader.git"
  },
  "keywords": [
    "node.js",
    "icu",
    "full-icu",
    "i18n",
    "intl",
    "internationalization"
  ],
  "author": "Masoud Ghorbani <msud.ghorbani@gmail.com> (http://msud.ir/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msudgh/icu-loader/issues"
  },
  "homepage": "https://github.com/msudgh/icu-loader#readme",
  "dependencies": {
    "full-icu": "^1.0.3",
    "lodash.isdate": "^4.0.1",
    "lodash.isnumber": "^3.0.3",
    "lodash.isstring": "^4.0.1"
  },
  "devDependencies": {
    "jest-cli": "^18.0.0",
    "sloc": "^0.1.11",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0"
  }
}
