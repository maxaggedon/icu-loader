{
  "name": "icu-loader",
  "version": "1.0.0",
  "description": "A package to load full ICU data in Node.js environment with JavaScript Intl API",
  "main": "index.js",
  "scripts": {
    "lint": "snazzy ./src/*.js index.js; true",
    "lint:fix": "npm run build && snazzy ./src/*.js index.js --fix; true",
    "build": "WEBPACK_ENV=production webpack; true",
    "dev": "WEBPACK_ENV=development webpack --progress --colors --watch --display-error-details; true",
    "stats-loc": "echo 'Source code stats:' && sloc --format cli-table ./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msudgh/icu-loader.git"
  },
  "keywords": [
    "node.js",
    "icu",
    "full-icu",
    "i18n",
    "intl",
    "internationalization"
  ],
  "author": "Masoud Ghorbani <msud.ghorbani@gmail.com> (http://msud.ir/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msudgh/icu-loader/issues"
  },
  "homepage": "https://github.com/msudgh/icu-loader#readme",
  "dependencies": {
    "full-icu": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "flow-bin": "^0.37.0",
    "flow-typed": "^2.0.0",
    "lodash": "^4.17.2",
    "sloc": "^0.1.11",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0",
    "webpack": "^1.14.0"
  }
}
